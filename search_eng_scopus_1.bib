title={"Hang on a sec, teacher": Collaborative and individual strategic planning as a means to perform L2 oral tasks at a public school ["SÓ mais um minutinho, teacher": Planejamento estratégico colaborativo e individual para tarefas orais em L2 em uma escola pública]},
journal={Ilha do Desterro},
year={2019},
volume={72},
number={3},
pages={401-425},
doi={10.5007/2175-8026.2019v72n3p401},
