@ARTICLE{Wilson2020454,
author={Wilson, E.O.P. and de Oliveira, J. and de Almeida, J.B. and Rodrigues, R.R.},
title={Individual differences and feedback effects in the acquisition of past tense verbs in Spanish as a foreign language [Diferenças individuais e efeitos de feedback na aquisição de verbos no pretérito em espanhol como L2]},
journal={Caracol},
year={2020},
number={19},
pages={454-481},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85089883320&partnerID=40&md5=eeeff6113cbbacac25f5b75fe0553687},
affiliation={Universidade Federal de Goiás (UFG), Brazil; Universidade Federal do Rio Grande do Sul (UFRGS), Brazil; Instituto Federal Goiano (Campus Ceres), Brazil},
abstract={This article presents a study that aimed to observe the effects of the feedback in the teaching of Spanish as a second language and its correlations with individual differences in learners' working memory and attention capacities. Participants were divided into two groups and had a period of formal instruction on the uses of past in Spanish, with each group receiving a different type of feedback (implicit or explicit). Respectively, preceded and proceeded the instruction, pretest and posttest, which included three linguistic tasks: judgment of grammaticality, phrasal comprehension, and phrasal production. There were performed tests to measure the attention and working memory capacities too. The results: the group that received explicit feedback increased their scores in the posttest in all tasks, and the group that received implicit feedback only increased the means of judgment of grammaticality and production. They showed correlations between the individual differences and feedback effects, mainly between working memory capacity and participants' performance. © Universidade de Sao Paulo. All rights reserved.},
author_keywords={Attention;  Explicit feedback;  Formal instruction;  Implicit Feedback;  Working memory},
