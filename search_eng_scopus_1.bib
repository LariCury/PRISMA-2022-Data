@ARTICLE{GomesJunior201857,
author={Gomes Junior, R.C. and Teixeira, G.S. and da Silva, M.G. and Paulino, C.M.A.},
title={Affordances of digital technologies for the development of oral skills in English [Affordances de tecnologias digitais para o desenvolvimento de habilidades orais em inglês]},
journal={Revista Brasileira de Linguistica Aplicada},
year={2018},
volume={18},
number={1},
pages={57-78},
doi={10.1590/1984-6398201812398},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85045046299&doi=10.1590%2f1984-6398201812398&partnerID=40&md5=7d83e612ff4e8df09892fdd89970cf70},
affiliation={Universidade Federal de Minas Gerais - UFMG, Belo Horizonte, Minas Gerais, Brazil},
abstract={Both in face-to-face and distance learning, the development of oral skills in foreign languages faces many difficulties in the Brazilian educational context, which indicates a great demand for effective ways of teaching and learning oral English skills. From an ecological perspective, this study aimed at investigating the affordances of digital technologies for developing oral skills in English through the perceptions of Languages and Literature students of the Federal University of Minas Gerais that used internet tools and apps to do weekly assignments from an online course. For this group of learners, the digital interactive technologies allowed them not only to plan and (re)record their oral production, but also to practice the oral comprehension, being capable of identifying their mistakes and difficulties. © 2018, Universidade Federal de Minas Gerais, Faculdade de Letras. All rights reserved.},
author_keywords={Affordances;  Digital technologies;  English language;  Oral skills},
funding_details={Conselho Nacional de Desenvolvimento Científico e TecnológicoConselho Nacional de Desenvolvimento Científico e Tecnológico, CNPq},
