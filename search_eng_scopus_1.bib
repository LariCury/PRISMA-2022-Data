@ARTICLE{Orfanò2018207,
author={Orfanò, B.M. and Oliveira, A.L.A.M. and da Silva, S.B.},
title={The role of pragmatic markers in academic spoken interlanguage: A corpus-based study of a group of brazilian efl university students [O papel dos marcadores pragmáticos na interlíngua falada acadêmica: UM estudo baseado em corpus de um grupo de estudantes universitários brasileiros de ile]},
journal={Diacritica},
year={2018},
volume={32},
number={3},
pages={207-226},
doi={10.21814/DIACRITICA.579},
note={cited By 0},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85108368496&doi=10.21814%2fDIACRITICA.579&partnerID=40&md5=d4bbde52a259bf1c9e7769454ee433df},
affiliation={Faculdade de Letras - Universidade Federal de Minas Gerais, Brazil; Departamento de Estatística, Universidade Federal de Ouro Preto, Brazil},
abstract={The present work addresses a group of university students of EFL (English as a Foreign Language) on how they use pragmatic markers in their oral productions. The initial hypothesis was that there would be differences both in usage and form in comparison to native speakers. In order to verify our claim, we set off to investigate two corpora: a learner oral corpus being compiled at the Federal University of Minas Gerais/Brazil and a sub-corpus from the British Academic Spoken English (BASE). While Brazilian students overuse items such as maybe and just, the data recorded in the UK displayed a more varied range of markers and multiword forms. Overall, the findings reinforce the importance of analyzing empirical data for a broader understanding of how native speakers and learners can differ in their oral academic production. The paper also sheds light on language teaching and learning in the academic setting from a pragmatic viewpoint. © 2018 Diacritica.},
author_keywords={Academic discourse;  Learner corpus;  Politeness},
