@ARTICLE{Windle201939,
author={Windle, J. and Miller, J.},
title={Scaffolding second language literacy: A model for students with interrupted schooling},
journal={European Journal of Applied Linguistics and TEFL},
year={2019},
volume={8},
number={1},
pages={39-59},
note={cited By 2},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-85105561406&partnerID=40&md5=6312a7ac15e441dc982f2893e1be4634},
affiliation={Fluminense Federal University, Institute of Modern Languages, Brazil; Monash University, Faculty of Education, Australia},
abstract={The difficulties facing English Language Learners who lack basic literacy skills in a first or other language in their teenage years, often stemming from interrupted histories of schooling, are manifold. Young refugees who attend school in a new country and new language, have typically fled conflict, lost family members, experienced considerable trauma and spent years in refugee camps. The present article introduces a model to help those working with such students to build literacy teaching strategies into their teaching. Based on a three-year research project in Australian schools, the model contributes to previous models of literacy and language learning by highlighting the importance of recycling and ex-panding L2 knowledge at different points in a learning and teaching cycle. The model is useful for guid-ing teachers to consciously develop both second language learning and literacy across a range of curriculum areas. © 2019, LinguaBooks. All rights reserved.},
author_keywords={Interrupted formal education;  Refugee-background students;  Scaffolding;  Second language literacy},
funding_details={Australian Research CouncilAustralian Research Council, ARC},
funding_text 1={Our research sought to identify specific student needs and areas for attention in teacher practice that are relevant in rethinking pedagogical models in the context of Australian secondary schooling (students aged 12-18). Supported by a grant from the Australian Research Council and working with the Victorian Department of Education and Early Childhood Development, we undertook a survey of secondary teachers working in schools receiving large numbers of refugee-background students (n=61) and conducted three case studies based on classroom observation in specialist classes for this cohort (6 classes, total of 65 students), interviews with students (n=18) and experienced teachers (n=8). This involved analysis of student work and testing of literacy abilities of the students. The classes were separate from “mainstream” students for part of the school day and were guided by both subject-area specialists and ESL teachers, including for mathematics and science at Year 9 level (students aged around 15 in the Australian education system). Students in these classes had already spent six months to a year in an English Language Centre, and hence were not experiencing Australian classrooms for the first time and brought with them conversational English sufficient for most everyday interactions and simple classroom routines.},
