@ARTICLE{deAlencarAbreu20211,
author={de Alencar Abreu, M.K. and Ximenes, V.M.},
title={Poverty, University Permanence and Student Support: A psychosocial analysis [Pauvreté, permanence universitaire et aide aux étudiants: Une analyse psychosociale] [Pobreza, permanência de universitários e assistência estudantil: Uma análise psicossocial] [La pobreza, la permanencia en la universidad y la asistencia a los estudiantes: Un análisis psicosocial]},
journal={Psicologia USP},
