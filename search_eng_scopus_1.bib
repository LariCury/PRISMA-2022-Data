@ARTICLE{Simon2016719,
author={Simon, E. and Lima, R., Jr. and De Cuypere, L.},
title={Acquiring non-native speech through early media exposure: Belgian children's productions of english vowels},
journal={Poznan Studies in Contemporary Linguistics},
year={2016},
volume={52},
number={4},
pages={719-743},
doi={10.1515/psicl-2016-0028},
note={cited By 1},
url={https://www.scopus.com/inward/record.uri?eid=2-s2.0-84997542945&doi=10.1515%2fpsicl-2016-0028&partnerID=40&md5=fde9464e8c6138dcf72b89907ffe3b0a},
affiliation={Linguistics Department, Ghent University, Blandijnberg 2, Gent, 9000, Belgium; Federal University of Ceará, Brazil},
abstract={This study aims to map native Dutch and non-native English vowels of Belgian children who have not been immersed and have not received instruction in English, but who are exposed to it through the media. It investigates to what extent this type of exposure is sufficient to develop new phonetic vowel categories. Twenty-four children aged 9-12 years performed production tasks focusing on Dutch and English monophthongs. Vowel formants were normalized and statistically analysed, and results highlight the English contrasts /ϵ-æ/, /∪-u/ and /α-∩/, which are lacking in Dutch. The children produced contrasting /ϵ/ and /æ/ in F1 and F2 in a repetition task, and English /ϵ/ and /æ/ were considerably different from the closest Dutch vowel /ϵ/ in terms of anteriority. The children's /∪-u/ and /α-∩/ differed in F1 and F2. The closest Dutch vowel /u/ did not differ from English /u/, and differed from /∪/ only in F1. Dutch /∩/ differed from /α/ in F1 and F2 and differed from English /∩/ in F1. The results suggest that media-induced Second Language Acquisition should not be underestimated, for even in contexts of L2 acquisition exclusively through media exposure, children learn to produce contrasts between L2 vowels which do not exist in their L1. © Faculty of English, Adam Mickiewicz University, Poznań, Poland 2016.},
author_keywords={child phonology;  Dutch;  English;  Second language acquisition;  vowels},
references={Alvarez, E., Rate and route of acquisition in EFL narrative development at different ages (2006) Age and the Rate of Foreign Language Learning, pp. 127-155. , Munoz, C. (ed.), Clevedon, Tonawanda, NY & Ontario: Multilingual Matters Ltd; Arantes, P., (2010) Formants.praat, V. 0.9 Beta; Arantes, P., (2011) Collect Formants.praat, V. 0.11 Alpha; Balota, D.A., Yap, M.J., Cortese, M.J., Hutchison, K.A., Kessler, B., Loftis, B., Neely, J.H., Treiman, R., The english lexicon project (2007) Behavior Research Methods, 39, pp. 445-459; Bolton, K., (2004) Chinese Englishes, , Cambridge: Cambridge University Press; Boersma, P., Weenink, D., (2011) Praat: Doing Phonetics by Computer [Computer Programme], , http://www.praat.org; Bohn, O.-S., Bundgaard-Nielsen, R.L., Second language speech learning with diverse input (2007) Input Matters in SLA, pp. 207-218. , Piske, T. and M. Young-Scholten (eds.). Clevedon: Multilingual Matters; Broersma, M., Perception of familiar contrasts in unfamiliar positions (2005) Journal of the Acoustical Society of America, 117, pp. 3890-3901; Chladkova, K., Hamann, S., High vowels in standard british english: /U/-fronting does not result in merger (2011) Proceedings of the 17th International Congress of Phonetic Sciences, pp. 476-479. , Lee, W.-S. and E. Zee (eds.), Hong Kong; Conboy, B.T., Brooks, R., Meltzoff, A.N., Kuhl, P., Social interaction in infants' learning of second language phonetics: An exploration of brain-behavior relations (2015) Developmental Neuropsychology, 40 (4), pp. 216-229; Crystal, D., (2003) English As a Global Language, , Cambridge: Cambridge University Press; Cutler, A., Broersma, M., Phonetic precision in listening (2005) A Figure of Speech: A Festschrift for John Laver, pp. 63-91. , Hardcastle, W.J. and J. Mackenzie Beck (eds.), London: Lawrence Earlbaum, London; Darcy, I., Kruger, F., Vowel perception and production in Turkish children acquiring L2 German (2012) Journal of Phonetics, 40, pp. 568-581; Dunn, L.M