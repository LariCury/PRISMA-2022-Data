title={Building the dialogue in English classes: An enunciative-discursive analysis of the interlocutions [A construção do diálogo em sala de aula de língua inglesa: Uma análise enunciativo-discursiva das interlocuções]},
journal={Calidoscopio},
year={2017},
volume={15},
number={1},
