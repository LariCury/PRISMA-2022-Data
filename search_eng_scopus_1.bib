@ARTICLE{Silva2018,
author={Silva, L.M. and de Souza, A.M.R. and de Almeida, F.S.},
title={Language, literature and identity construction in pedagogical practices: Foreign language teaching in a resistance perspective [Linguagem, literatura e construção de identidades em práticas pedagógicas: O ensino de línguas estrangeiras em uma perspectiva de resistência] [Lenguaje, literatura y construcción de identidades en prácticas pedagógicas: La enseñanza de lenguas extranjeras en una perspectiva de resistencia]},
