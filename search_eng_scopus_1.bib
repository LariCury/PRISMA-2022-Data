@ARTICLE{Rojo201517,
author={Rojo, R.A.O. and Gonzalez, I.V.D.P. and Sartório, A. and de Oliveira, L.C.},
title={Social capital in a globalized world: A study on the fostering and acquisition in administration courses [Capital social no mundo globalizado: Um estudo acerca da fomentação e formas de aquisição em cursos superiores de administração]},
